		{
			"name": "Harga dan Slot",
			"item": [
				{
					"name": "TC 04 Booking Cek Harga",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code harus 201\", function () {\r",
									"    pm.response.to.have.status(201);\r",
									"});\r",
									"\r",
									"const res = pm.response.json();\r",
									"const start = res.start_time;\r",
									"const end = res.end_time;\r",
									"const actualPrice = res.price;\r",
									"\r",
									"function getExpectedPrice(start, end) {\r",
									"    if (start === \"07:00:00\" && end === \"09:00:00\") {\r",
									"        return 800000;\r",
									"    } else if (start === \"09:00:00\" && end === \"11:00:00\") {\r",
									"        return 1000000;\r",
									"    } else if (start === \"11:00:00\" && end === \"13:00:00\") {\r",
									"        return 1200000;\r",
									"    } else {\r",
									"        return null;\r",
									"    }\r",
									"}\r",
									"const expectedPrice = getExpectedPrice(start, end);\r",
									"\r",
									"if (expectedPrice === null) {\r",
									"    pm.test(\"Slot waktu tidak valid\", function () {\r",
									"        pm.expect.fail(`Waktu booking ${start}â€“${end} tidak ditemukan di jadwal.`);\r",
									"    });\r",
									"} else {\r",
									"    pm.test(`Harga booking harus sesuai (${expectedPrice})`, function () {\r",
									"        pm.expect(actualPrice).to.eql(expectedPrice);\r",
									"    });\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
